<view class="page">
  <!--如果是销售代表显示  -->
  <view class="page__bd" wx:if='{{isSalesRep==="true"}}'>
    <view class="weui-flex home-flex-wrap">
      <view class='home-item' style=' border-right: {{(index+1)%2==0?0: "1rpx solid #d9d9d9"}}' wx:for="{{grids}}" wx:key="*this" wx:item='item' wx:index='index'>
        <navigator url="{{item.router}}" hover-class="weui-grid_active">
          <image class="weui-grid__icon" src="{{item.url}}" />
          <view class="weui-grid__label">
            <text class='item-title'>{{item.type}}</text>
          </view>
        </navigator>
      </view>
    </view>
  </view>
  <!--如果不是销售代表  -->
  <image style='width:100%;height:100vh' wx:if='{{isSalesRep==="false"}}' src='https://personerp.oss-cn-hangzhou.aliyuncs.com/datas/serviceSales/index.gif' bindtap='deputyHome'></image>
</view>