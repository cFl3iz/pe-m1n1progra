<view class="page">
  <view class="page__bd">
    <view class="weui-grids">
      <block wx:for="{{grids}}" wx:key="*this" wx:item='item'>
        <navigator url="{{item.router}}" class="weui-grid" style='width:50%;background-color:{{item.backgroundColor}}' hover-class="weui-grid_active">
            <image class="weui-grid__icon" src="{{item.url}}" />
            <view class="weui-grid__label">{{item.type}}</view>
        </navigator>
      </block>
    </view>
  </view>
</view>